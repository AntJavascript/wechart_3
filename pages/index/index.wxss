.container {
    padding: 0;
}
.no-login {
    width:100%;
    height:100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.no-login .reg-teacher, .no-login .reg-user {
    border: 1px solid #1989fa;
    color: #1989fa;
    border-radius: 8px;
    display: block;
    margin: 20px;
    padding: 16px 100px;
    font-size: 18px
}
.no-login .reg-teacher .icon, .no-login .reg-user .icon {
    margin-right:14px;
}
